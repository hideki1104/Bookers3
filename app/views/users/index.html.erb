<%= render "users/list",user:@user,book:@book %>

<div class="container">
<div class="row">
<div class="col-xs-9">
<h2>Users</h2>
<table class="table table-striped">
  <thead>
  	<th>Image</th>
  	<th>Name</th>
  </thead>
  <% @users.each do |user| %>
  <tbody>
  	<tr>
      <td>
      	<%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpeg',fallback: "no_image.jpg" %> 
      </td>
      <td>
      	  <%= user.name %>
      </td>
      <td>
        <%= link_to "Show",user_path(user.id) %>
      </td>
  	</tr>
  </tbody>
<% end %>
</table>
</div>
</div>
</div>